{
  "name": "@adaptavant/eds-core",
  "version": "1.19.0",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "module": "./dist/adaptavant-eds-core.esm.js",
      "default": "./dist/adaptavant-eds-core.cjs.js"
    },
    "./icons": {
      "module": "./icons/dist/adaptavant-eds-core-icons.esm.js",
      "default": "./icons/dist/adaptavant-eds-core-icons.cjs.js"
    },
    "./utils": {
      "module": "./utils/dist/adaptavant-eds-core-utils.esm.js",
      "default": "./utils/dist/adaptavant-eds-core-utils.cjs.js"
    },
    "./tokens": {
      "module": "./tokens/dist/adaptavant-eds-core-tokens.esm.js",
      "default": "./tokens/dist/adaptavant-eds-core-tokens.cjs.js"
    },
    "./symbols": {
      "module": "./symbols/dist/adaptavant-eds-core-symbols.esm.js",
      "default": "./symbols/dist/adaptavant-eds-core-symbols.cjs.js"
    },
    "./widgets": {
      "module": "./widgets/dist/adaptavant-eds-core-widgets.esm.js",
      "default": "./widgets/dist/adaptavant-eds-core-widgets.cjs.js"
    },
    "./tailwind": {
      "module": "./tailwind/dist/adaptavant-eds-core-tailwind.esm.js",
      "default": "./tailwind/dist/adaptavant-eds-core-tailwind.cjs.js"
    },
    "./experimental": {
      "module": "./experimental/dist/adaptavant-eds-core-experimental.esm.js",
      "default": "./experimental/dist/adaptavant-eds-core-experimental.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/adaptavant-eds-core.cjs.js",
  "module": "dist/adaptavant-eds-core.esm.js",
  "files": [
    "dist",
    "experimental",
    "icons",
    "symbols",
    "tailwind",
    "tokens",
    "utils",
    "widgets"
  ],
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.1",
    "@react-aria/focus": "^3.18.1",
    "@react-aria/utils": "^3.25.1",
    "@tanstack/react-table": "^8.19.3",
    "clsx": "^2.1.1",
    "compressorjs": "^1.2.1",
    "cropperjs": "^1.6.2",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "emery": "^1.4.3",
    "lodash": "^4.17.21",
    "postcss-cli": "^11.0.0",
    "react-day-picker": "^8.10.1",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@adaptavant-internal/babel-preset": "0.0.0",
    "@adaptavant-internal/eslint-config": "0.0.0",
    "@adaptavant-internal/locale": "0.0.0",
    "@adaptavant-internal/tsconfig": "0.0.0",
    "@preconstruct/cli": "^2.8.7",
    "@types/cropperjs": "^1.3.0",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.2.72",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "clean-package": "^2.2.0",
    "eslint": "^8.57.0",
    "match-sorter": "^6.3.4",
    "postcss": "^8.4.38",
    "postcss-load-config": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "clean-package": {
    "remove": [
      "scripts.postinstall"
    ]
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "experimental/index.ts",
      "icons/index.ts",
      "symbols/index.ts",
      "tailwind/index.ts",
      "tokens/index.ts",
      "utils/index.ts",
      "widgets/index.ts"
    ],
    "exports": true
  },
  "scripts": {
    "build": "preconstruct build",
    "check:types": "tsc --noEmit",
    "clean": "pnpm clean:outputs && rm -rf node_modules",
    "clean:outputs": "rm -rf .turbo && rm -rf dist && rm -rf src/icons && rm -rf experimental/dist && rm -rf icons/dist && rm -rf tailwind/dist && rm -rf tokens/dist && rm -rf utils/dist",
    "generate:class-list": "postcss src/tailwind/generate-class-list/style.css -o dist/tailwind/generate-class-list/compiled.css && tsx src/tailwind/generate-class-list/generate-class-list.ts",
    "lint": "TIMING=1 eslint \"src/**/*.{js,ts,jsx,tsx}\" --max-warnings 0 --report-unused-disable-directives",
    "test": "vitest"
  }
}